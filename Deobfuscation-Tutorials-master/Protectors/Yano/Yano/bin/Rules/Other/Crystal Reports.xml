<?xml version="1.0" encoding="utf-8"?>
<yano xmlns="urn:yano" version="1.0">
  <rules description="Crystal Reports">

    <rule name="Crystal Reports: Disable renaming classes generated by Crystal Reports">
      <renaming enabled="false" />
      <apply-to-type specifiers="class">
        <having-type named="CrystalDecisions.CrystalReports.Engine.ReportObject" />
      </apply-to-type>
    </rule>

  </rules>
</yano>